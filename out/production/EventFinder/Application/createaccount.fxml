<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171"
  xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Createaccount">
  <Text fx:id="usernameText" layoutX="121.0" layoutY="106.0" strokeType="OUTSIDE" strokeWidth="0.0"
    text="Username:"/>
  <Text fx:id="newPasswordText" layoutX="122.0" layoutY="154.0" strokeType="OUTSIDE"
    strokeWidth="0.0" text="New Password:"/>
  <Text fx:id="confirmPasswordText" layoutX="121.0" layoutY="205.0" strokeType="OUTSIDE"
    strokeWidth="0.0" text="Confirm Password:"/>
  <TextField fx:id="usernameField" layoutX="263.0" layoutY="89.0"/>
  <TextField fx:id="newPassword" layoutX="263.0" layoutY="137.0"/>
  <TextField fx:id="confirmPassword" layoutX="263.0" layoutY="188.0"/>
  <Button fx:id="createAccountButton" layoutX="457.0" layoutY="267.0" mnemonicParsing="false"
    onAction="#createAccount" text="Create Account"/>
  <Button fx:id="backButton" layoutX="457.0" layoutY="316.0" mnemonicParsing="false"
    onAction="#handleButton" text="Back"/>
</AnchorPane>
