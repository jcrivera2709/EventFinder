    <font>
      <Font name="SansSerif Italic" size="24.0"/>
    </font>
  </Text>
  <TextField fx:id="eventLocationField" layoutX="225.0" layoutY="139.0" prefHeight="25.0"
    prefWidth="165.0"/>
  <Text fx:id="eventDateText" fontSmoothingType="LCD" layoutX="14.0" layoutY="201.0"
