  <TextArea fx:id="eventDescription" layoutX="14.0" layoutY="295.0" prefHeight="123.0"
    prefWidth="261.0"/>
  <Text fx:id="eventNameTxt" fontSmoothingType="LCD" layoutX="14.0" layoutY="124.0"
