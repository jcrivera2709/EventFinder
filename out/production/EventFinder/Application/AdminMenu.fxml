<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171"
  xmlns:fx="http://javafx.com/fxml/1" fx:controller="Application.AdminMenu">
  <children>
    <ComboBox fx:id="categoryBox" layoutX="390.0" layoutY="102.0" prefHeight="27.0"
      prefWidth="163.0" promptText="Choose Category"/>
    <Button fx:id="addEventButton" layoutX="406.0" layoutY="215.0" mnemonicParsing="false"
      onAction="#addEvent" text="Add Event"/>
    <TextField fx:id="eventNameField" layoutX="163.0" layoutY="102.0" prefHeight="25.0"
      prefWidth="200.0"/>
    <TextArea fx:id="eventDescription" layoutX="163.0" layoutY="176.0" prefHeight="200.0"
      prefWidth="200.0"/>
    <Text fx:id="eventNameTxt" fontSmoothingType="LCD" layoutX="14.0" layoutY="124.0"
      strokeType="OUTSIDE" strokeWidth="0.0" text="Event Name:">
      <font>
        <Font name="SansSerif Italic" size="24.0"/>
      </font>
    </Text>
    <Text fx:id="addEventTxt" fontSmoothingType="LCD" layoutX="163.0" layoutY="65.0"
      strokeType="OUTSIDE" strokeWidth="0.0" text="Add Event ">
      <font>
        <Font name="SansSerif Italic" size="56.0"/>
      </font>
    </Text>
    <Text fx:id="eventDescTxt" fontSmoothingType="LCD" layoutX="14.0" layoutY="171.0"
      strokeType="OUTSIDE" strokeWidth="0.0" text="Event Description:">
      <font>
        <Font name="SansSerif Italic" size="24.0"/>
      </font>
    </Text>
    <Button fx:id="eventsButton" layoutX="406.0" layoutY="276.0" mnemonicParsing="false"
      text="Go to Events" onAction="#handleButtonAction"/>
  </children>
</AnchorPane>
